{
    "name": "codicastudio/statistics",
    "description": "A noobs worthless garbage, just learning.",
    "keywords": [
        "worthless garabage by a noob."
    ],
    "homepage": "https://github.com/codicastudio/statistics",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "codicastudiodev@gmail.com",
            "homepage": "https://codicastudio.dev",
            "role": "Developer"
        }
    ],

    "require": {
        "php": "^7.4.0",
        "geoip2/geoip2": "^2.10.0",
        "illuminate/bus": "^7.0.0",
        "illuminate/console": "^7.0.0",
        "illuminate/contracts": "^7.0.0",
        "illuminate/database": "^7.0.0",
        "illuminate/http": "^7.0.0",
        "illuminate/queue": "^7.0.0",
        "illuminate/routing": "^7.0.0",
        "illuminate/support": "^7.0.0",
        "jenssegers/agent": "^2.6.0",
        "rinvex/countries": "^7.0.0",
        "rinvex/laravel-support": "^4.0.0",
        "symfony/http-foundation": "^5.0.0",
        "torann/geoip": "^1.2.0",
        "ua-parser/uap-php": "^3.8.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.27.0",
        "illuminate/container": "^7.0.0",
        "phpunit/phpunit": "^9.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "codicastudio\\Statistics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "codicastudio\\Statistics\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },

    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "codicastudio\\Statistics\\Providers\\StatisticsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
